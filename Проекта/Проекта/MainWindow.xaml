<Window x:Class="Проекта.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Проекта"
        mc:Ignorable="d"
        Title="MainWindow" Height="700" Width="1000">
    <Grid Background="White">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="200"/>
            <ColumnDefinition/>
            <ColumnDefinition Width="200"/>
        </Grid.ColumnDefinitions>
        <Image Grid.Column="1" Source="ADAMAS.png" OpacityMask="Black"/>
        <Rectangle Grid.Column="1" Height="85" Margin="10,299,10,285" Stroke="White" Fill="#7F303030"/>
        <Rectangle Grid.Column="1" Height="85" Fill="#7F303030" Margin="10,199,10,385" Stroke="White"/>
        <Rectangle Grid.Column="1" Height="85" Fill="#7F303030" Margin="10,99,10,485" Stroke="White"/>
        <Rectangle Grid.Column="1" Height="85" Fill="#7F303030" Margin="10,399,10,185" Stroke="White"/>
        <Rectangle Grid.Column="1" Height="85" Fill="#7F303030" Margin="10,499,10,85" Stroke="White"/>
        <Rectangle Grid.Column="0" Fill="#FF400000" />
        <Rectangle Grid.Column="2" Fill="#FF00402F"/>
    </Grid>
</Window>
